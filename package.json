{
  "name": "milkmidi-vue-jsx-storybook",
  "version": "1.0.0",
  "private": false,
  "author": "milkmidi <milkmidi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "plop": "plop --plopfile ./internal/generator/index.js",
    "storybook": "start-storybook -p 9001 -c ./internal/storybook",
    "storybook-build": "build-storybook -c ./internal/storybook -o dist/storybook",
    "test": "jest --json --outputFile=.jest-test-results.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milkmidi/2018_vue_jsx_storybook.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "ml.anteater/(.*)$": "<rootDir>/src/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true
  },
  "devDependencies": {
